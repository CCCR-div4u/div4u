apiVersion: v1
kind: Secret
metadata:
  name: harbor-registry-secret
type: kubernetes.io/dockerconfigjson
stringData:
  .dockerconfigjson: |
    {
      "auths": {
        "harbor.bluesunnywings.com": {
          "username": "YOUR_HARBOR_USERNAME",
          "password": "YOUR_HARBOR_PASSWORD",
          "auth": "BASE64_ENCODED_USERNAME_PASSWORD"
        }
      }
    }